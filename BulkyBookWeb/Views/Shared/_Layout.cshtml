    <script src="~/Content/Plugin/plugins/google-map/gmap.js"></script>

    <!-- Form Validation -->
    <script src="~/Content/Plugin/plugins/form-validation/jquery.form.js"></script>
    <script src="~/Content/Plugin/plugins/form-validation/jquery.validate.min.js"></script>

    <!-- Bootstrap4 -->
    <script src="~/Content/Plugin/plugins/bootstrap/js/bootstrap.min.js"></script>
    <!-- Parallax -->
    <script src="~/Content/Plugin/plugins/parallax/jquery.parallax-1.1.3.js"></script>
    <!-- lightbox -->
    <script src="~/Content/Plugin/plugins/lightbox2/dist/js/lightbox.min.js"></script>
    <!-- Owl Carousel -->
    <script src="~/Content/Plugin/plugins/slick/slick.min.js"></script>
    <!-- filter -->
    <script src="~/Content/Plugin/plugins/filterizr/jquery.filterizr.min.js"></script>
    <!-- Smooth Scroll js -->
    <script src="~/Content/Plugin/plugins/smooth-scroll/smooth-scroll.min.js"></script>

    <!-- Custom js -->
    <script src="~/Content/Plugin/js/script.js"></script>
    @RenderSection("scripts", required: false)
